	"heroVoice":[			
		{"voiceid":1,"herovoice":1,"type":1,"herotext":"澤門之皙，實興我役，邑中之黔，實慰我心。"},		
		{"voiceid":10,"herovoice":10,"type":1,"herotext":"銷甲事春農，萬里孤征，終古謀謨齊稷契。"},		
		{"voiceid":20,"herovoice":20,"type":1,"herotext":"敏而好學可為文，授之以政無不達。"},		
		{"voiceid":30,"herovoice":30,"type":1,"herotext":"泥芹有宿根，一寸嗟獨在，雪芹何時動，春鳩行可膾。"},		
		{"voiceid":33,"herovoice":33,"type":1,"herotext":"記住，我是禮親王，兩紅旗的旗主！"},		
		{"voiceid":40,"herovoice":40,"type":1,"herotext":"人生若只如初見，何事秋風悲畫扇。"},		
		{"voiceid":50,"herovoice":50,"type":1,"herotext":"千古英雄死遺恨，封侯廟食更何心。"},		
		{"voiceid":60,"herovoice":60,"type":1,"herotext":"禮樂法而不說，詩書故而不切，春秋約而不速。"},		
		{"voiceid":70,"herovoice":70,"type":1,"herotext":"四時不謝之蘭，百節長青之竹，萬古不敗之石，千秋不變之人。"},		
		{"voiceid":80,"herovoice":80,"type":1,"herotext":"今後，這宮裡生活不比外頭，無論在哪裡都需如履薄冰。"},		
		{"voiceid":81,"herovoice":81,"type":2,"herotext":"若有什麼難出，儘管和我說便是。"},		
		{"voiceid":82,"herovoice":82,"type":2,"herotext":"沒……沒什麼，只是想來看看你。"},		
		{"voiceid":83,"herovoice":83,"type":2,"herotext":"今日氣候適宜，走，陪本王射箭去！"},		
		{"voiceid":90,"herovoice":90,"type":1,"herotext":"國家大事重於一切，真難得，你居然能有此覺悟。"},		
		{"voiceid":91,"herovoice":91,"type":1,"herotext":"百姓安危顧然重要，但你只是個弱女子，莫要逞強。"},		
		{"voiceid":92,"herovoice":92,"type":1,"herotext":"你啊你，何時才能讓本王省點心。"},		
		{"voiceid":93,"herovoice":93,"type":1,"herotext":"若是你願意，本王可當你永遠的後盾，為你遮風擋雨。"},		
		{"voiceid":100,"herovoice":100,"type":1,"herotext":"等閒識得東風面，萬紫千紅總是春……"},		
		{"voiceid":101,"herovoice":101,"type":2,"herotext":"天涼，可別在外頭待太久……"},		
		{"voiceid":102,"herovoice":102,"type":2,"herotext":"只是許久不見你，想看看你近來過得如何……"},		
		{"voiceid":103,"herovoice":103,"type":2,"herotext":"愛也好，恨也罷，就讓時間來證明吧。"},		
		{"voiceid":110,"herovoice":110,"type":1,"herotext":"今日氣候適宜，姑娘可願陪本王一同前往靈囿欣賞南國孔雀？"},		
		{"voiceid":111,"herovoice":111,"type":1,"herotext":"本王前些時日贈與你的青鳳裘，你可還喜歡？"},		
		{"voiceid":112,"herovoice":112,"type":1,"herotext":"你總是能看穿我的所思所想，是本王低估你了。"},		
		{"voiceid":113,"herovoice":113,"type":1,"herotext":"把手給我，本王帶你去看看這冬日的第一場雪。"},		
		{"voiceid":120,"herovoice":120,"type":1,"herotext":"怎麼，不相信本王？"},		
		{"voiceid":121,"herovoice":121,"type":2,"herotext":"哈哈哈哈，這幾日不見，你可曾思念我？"},		
		{"voiceid":122,"herovoice":122,"type":2,"herotext":"本王的心，永遠留有你的位置。"},		
		{"voiceid":123,"herovoice":123,"type":2,"herotext":"這初見平平無奇，可你越看…卻越讓本王心動。"},		
		{"voiceid":130,"herovoice":130,"type":1,"herotext":"你可別太擔心，本王一向對女人溫柔。"},		
		{"voiceid":131,"herovoice":131,"type":1,"herotext":"我對你的真心，你難道看不出來麼？"},		
		{"voiceid":132,"herovoice":132,"type":1,"herotext":"告訴本王，想要佔據你的心，本王……還該做些什麼？"},		
		{"voiceid":133,"herovoice":133,"type":1,"herotext":"認命吧，你的整個人整個心，終將屬於本王。"},		
		{"voiceid":140,"herovoice":140,"type":1,"herotext":"願有一日，可與哥哥們一同把酒言歡。"},		
		{"voiceid":141,"herovoice":141,"type":2,"herotext":"本王平日舞刀弄槍慣了，可沒嚇到姑娘吧。"},		
		{"voiceid":142,"herovoice":142,"type":2,"herotext":"姑娘可知，本王久經沙場，生死早已置之度外。"},		
		{"voiceid":143,"herovoice":143,"type":2,"herotext":"和你說說話，本王這心裡便暢快多了。"},		
		{"voiceid":150,"herovoice":150,"type":1,"herotext":"我還是頭一次，見到像你這麼好看的姑娘。"},		
		{"voiceid":151,"herovoice":151,"type":1,"herotext":"舞刀弄槍的，我可是最拿手了。別怕，我保護你！"},		
		{"voiceid":152,"herovoice":152,"type":1,"herotext":"若是你想，本王也可放下刀槍，陪你一同賞花看畫。"},		
		{"voiceid":153,"herovoice":153,"type":1,"herotext":"這江山，以前是為了八哥守著，現在，我想也替你守著。"},		
		{"voiceid":160,"herovoice":160,"type":1,"herotext":"今兒有空嗎？本王想聽你彈奏一曲《鳳求凰》…"},		
		{"voiceid":162,"herovoice":162,"type":2,"herotext":"姑娘是不是也覺得？八哥很厲害！"},		
		{"voiceid":163,"herovoice":163,"type":2,"herotext":"你這眉頭都快擰成球了，快給本王笑一個！"},		
		{"voiceid":164,"herovoice":164,"type":2,"herotext":"其實這宮裡啊，也沒你想的這麼恐怖，這不？有我在呢！"},		
		{"voiceid":170,"herovoice":170,"type":1,"herotext":"好一個巾幗英雄，本王深感欽佩！"},		
		{"voiceid":171,"herovoice":171,"type":1,"herotext":"這天天聽八哥說你的故事，我的耳朵都起繭了。"},		
		{"voiceid":172,"herovoice":172,"type":1,"herotext":"姑娘既為八哥之妹，那也是本王家人，自當為你傾盡所有。"},		
		{"voiceid":173,"herovoice":173,"type":1,"herotext":"不曾想，我一向寡淡的生命裡，竟闖進了你……"},		
		{"voiceid":180,"herovoice":180,"type":1,"herotext":"你總算來了，趕緊的！陪本王賭上幾把！"},		
		{"voiceid":181,"herovoice":181,"type":2,"herotext":"這日子閑，小賭怡情，小賭怡情嘛！"},		
		{"voiceid":182,"herovoice":182,"type":2,"herotext":"本王當你是朋友，才跟你說這些的，你可別不領情！"},		
		{"voiceid":183,"herovoice":183,"type":2,"herotext":"你今兒氣色不錯嘛！可有什麼好事說來聽聽~"},		
		{"voiceid":190,"herovoice":190,"type":1,"herotext":"凡事莫要衝動，有事可找本王商量。"},		
		{"voiceid":191,"herovoice":191,"type":1,"herotext":"這皇宮裡人才輩出，本王一身才情，也只能給你看了。"},		
		{"voiceid":192,"herovoice":192,"type":1,"herotext":"想來這世間，竟沒人如你般瞭解本王了。"},		
		{"voiceid":193,"herovoice":193,"type":1,"herotext":"本王賭了這麼久，何時才能賭一把……你的心？"},		
		{"voiceid":200,"herovoice":200,"type":1,"herotext":"這有些事啊，可沒你想的那麼簡單。"},		
		{"voiceid":201,"herovoice":201,"type":2,"herotext":"本王只不過想，一生一世一雙人罷了。"},		
		{"voiceid":202,"herovoice":202,"type":2,"herotext":"醉過方知酒濃，愛過方知情重。"},		
		{"voiceid":203,"herovoice":203,"type":2,"herotext":"若她有你對我一半好，那該多好……"},		
		{"voiceid":210,"herovoice":210,"type":1,"herotext":"這一次，我是真的要忘了。"},		
		{"voiceid":211,"herovoice":211,"type":1,"herotext":"是你讓我忘卻了那些不高興的事，說來…還得感謝你呢。"},		
		{"voiceid":212,"herovoice":212,"type":1,"herotext":"你為何，總是出現在本王的心頭？"},		
		{"voiceid":213,"herovoice":213,"type":1,"herotext":"這寒冬夜本該覺得寒冷，但有你在本王身旁，竟覺得甚是溫暖。"},		
		{"voiceid":220,"herovoice":220,"type":1,"herotext":"本王還有事要忙，姑娘，先回吧。"},		
		{"voiceid":221,"herovoice":221,"type":2,"herotext":"這一去便不知多久才能回來，可別太想我。"},		
		{"voiceid":222,"herovoice":222,"type":2,"herotext":"喏~才帶回來的桂花糕，快嘗嘗！"},		
		{"voiceid":223,"herovoice":223,"type":2,"herotext":"本王不在的這些時日，你可別多看其他男人！"},		
		{"voiceid":230,"herovoice":230,"type":1,"herotext":"姑娘，你可不能忘了本王幫過你的恩情啊。"},		
		{"voiceid":231,"herovoice":231,"type":1,"herotext":"本王真是有幸，竟結識了如此聰慧的女子。"},		
		{"voiceid":232,"herovoice":232,"type":1,"herotext":"本王決定，今後與姑娘患難與共了！"},		
		{"voiceid":233,"herovoice":233,"type":1,"herotext":"想不到本王的心，有一日也會因姑娘而狂跳不止。"},		
		{"voiceid":240,"herovoice":240,"type":1,"herotext":"嗯？你找我……有什麼事嗎？"},		
		{"voiceid":241,"herovoice":241,"type":2,"herotext":"嗯，來了？"},		
		{"voiceid":242,"herovoice":242,"type":2,"herotext":"人生如棋，這個局你可能破？"},		
		{"voiceid":243,"herovoice":243,"type":2,"herotext":"曲通人心，與你是，與我也是。"},		
		{"voiceid":250,"herovoice":250,"type":1,"herotext":"如果美色時時都有用的話，這宮裡的有些事，便簡單許多。"},		
		{"voiceid":251,"herovoice":251,"type":1,"herotext":"你放心，就算是為了我自己，我是不會丟下你一個人的。"},		
		{"voiceid":252,"herovoice":252,"type":1,"herotext":"可以多考慮考慮自己嗎，可……可別再受傷了！"},		
		{"voiceid":253,"herovoice":253,"type":1,"herotext":"本王的心……也是會因你而疼的。"},		
		{"voiceid":260,"herovoice":260,"type":1,"herotext":"你我二人今朝相見，竟已是這般陌生……"},		
		{"voiceid":261,"herovoice":261,"type":2,"herotext":"看這春風欲度桃花落，你我二人卻回不去了…"},		
		{"voiceid":262,"herovoice":262,"type":2,"herotext":"我不在的日子，你可要照顧好自己。"},		
		{"voiceid":263,"herovoice":263,"type":2,"herotext":"待我君臨天下，定賞你這一世的繁華。"},		
		{"voiceid":280,"herovoice":280,"type":1,"herotext":"行遍江南水與山，柳舒花放鳥綿蠻。"},		
		{"voiceid":290,"herovoice":290,"type":1,"herotext":"妻子豈應關大計?英雄無奈是多情！"},		
		{"voiceid":300,"herovoice":300,"type":1,"herotext":"治天下在得民心，士為秀民，士心得，則民心得矣。"},		
		{"voiceid":310,"herovoice":310,"type":1,"herotext":"本官掌儀天下之政，何人不服？"},		
		{"voiceid":320,"herovoice":320,"type":1,"herotext":"本王只想獨善其身，創作出更多美妙的樂曲。"},		
		{"voiceid":321,"herovoice":321,"type":2,"herotext":"這笛聲，你可還喜歡？"},		
		{"voiceid":322,"herovoice":322,"type":2,"herotext":"有些說不出口的話，笛聲可替本王訴說。"},		
		{"voiceid":323,"herovoice":323,"type":2,"herotext":"明日有場禮樂表演，不知姑娘可願陪本王同去？"},		
		{"voiceid":330,"herovoice":330,"type":1,"herotext":"快來，我新作了一首曲子，你聽聽，可還喜歡？"},		
		{"voiceid":331,"herovoice":331,"type":1,"herotext":"不知從何時起，第一時間就想和你分享好曲子。"},		
		{"voiceid":332,"herovoice":332,"type":1,"herotext":"原來，這便是心動的感覺……"},		
		{"voiceid":333,"herovoice":333,"type":1,"herotext":"你可知，這是本王第一次如此用心的對待一個女人。"},		
		{"voiceid":340,"herovoice":340,"type":1,"herotext":"有的人即使臉上在笑，心中的痛，卻是半分也不少的。"},		
		{"voiceid":350,"herovoice":350,"type":1,"herotext":"姑娘可是我見過的人中，最善良的那個。"},		
		{"voiceid":351,"herovoice":351,"type":1,"herotext":"這紫禁城，遠比想像中來的兇險，姑娘要多加小心。"},		
		{"voiceid":352,"herovoice":352,"type":1,"herotext":"屬下願格格一生一帆風順，再不受苦。"},		
		{"voiceid":353,"herovoice":353,"type":1,"herotext":"若可以，屬下願犧牲自己，為格格赴湯蹈火，在所不辭！"},		
		{"voiceid":360,"herovoice":360,"type":1,"herotext":"像你這樣，榮華富貴都入不了眼的姑娘，可真是稀奇。"},		
		{"voiceid":370,"herovoice":370,"type":1,"herotext":"你要知道，江山對我來說，遠比兒女情長重要得多。"},		
		{"voiceid":371,"herovoice":371,"type":1,"herotext":"你……能夠原諒我嗎？"},		
		{"voiceid":372,"herovoice":372,"type":1,"herotext":"只是夜深人靜的時候，時常會想起你罷了。"},		
		{"voiceid":373,"herovoice":373,"type":1,"herotext":"失去你的日子，本王夜夜無法安穩入眠。只怕你…是永遠回不來了。"},		
		{"voiceid":380,"herovoice":380,"type":1,"herotext":"這天下，沒有什麼是我得不到的。"},		
		{"voiceid":400,"herovoice":400,"type":1,"herotext":"喲，這太陽打西邊出來了呀，我當是誰呢？"},		
		{"voiceid":420,"herovoice":420,"type":1,"herotext":"賑糧與否，全憑本王心情，你能奈我何？"},		
		{"voiceid":430,"herovoice":430,"type":1,"herotext":"滿洲乃大清龍興之地，豈容蠻夷放肆！"},		
		{"voiceid":450,"herovoice":450,"type":1,"herotext":"有沒有興趣和我平分這個世界？"},		
		{"voiceid":460,"herovoice":460,"type":1,"herotext":"你，過來讓本王看看。"},		
		{"voiceid":480,"herovoice":0,"type":1,"herotext":"謙謙君子，卑以自牧也"},		
		{"voiceid":490,"herovoice":0,"type":1,"herotext":"我命由我，不由天！"},		
		{"voiceid":500,"herovoice":0,"type":1,"herotext":"不謀全域者，不足以謀一域"},		
		{"voiceid":510,"herovoice":0,"type":1,"herotext":"遇到危險時，就站到我的身後"},		
		{"voiceid":520,"herovoice":0,"type":1,"herotext":"見你面有愁容，最近可有什麼煩心事?"},		
		{"voiceid":530,"herovoice":0,"type":1,"herotext":"十裡長亭，望眼欲穿"},		
		{"voiceid":540,"herovoice":0,"type":1,"herotext":"君王是人，也有七情六欲。"},		
		{"voiceid":550,"herovoice":0,"type":1,"herotext":"  看到你，心情便明快了起來。"},		
		{"voiceid":560,"herovoice":0,"type":1,"herotext":"君子有諸己而後求諸人，無諸己而後非諸人"}		
	],			
	"wifeVoice":[			
		{"voiceid":1,"wifevoice":1,"type":1,"wifetext":"我只想和你策馬奔騰，共用塵世繁華！"},		
		{"voiceid":20,"wifevoice":20,"type":1,"wifetext":"我雖卑微，卻仍可助你一臂之力！"},		
		{"voiceid":21,"wifevoice":21,"type":1,"wifetext":"有時……我總覺得你是另一個我。"},		
		{"voiceid":22,"wifevoice":22,"type":1,"wifetext":"開心的時候要想起我，難過的時候也別忘了我。"},		
		{"voiceid":23,"wifevoice":23,"type":1,"wifetext":"你我二人相互扶持，這宮裡再苦再累，我都能受。"},		
		{"voiceid":40,"wifevoice":40,"type":1,"wifetext":"只要姑娘心裡有我一個位置，培盛就知足了。"},		
		{"voiceid":60,"wifevoice":60,"type":1,"wifetext":"就算是天定的良緣，也會很辛苦，我有心裡準備！"},		
		{"voiceid":80,"wifevoice":80,"type":1,"wifetext":"若非家門不幸，我也遇不到你，這也算禍兮福所依吧~"},		
		{"voiceid":100,"wifevoice":0,"type":1,"wifetext":"如果宗室都能如姑娘這般自律，天下何愁不太平？"},		
		{"voiceid":120,"wifevoice":0,"type":1,"wifetext":"哼，殿下這麼久不來看我，本宮不理你了！"},		
		{"voiceid":140,"wifevoice":0,"type":1,"wifetext":"天日昭昭，墜月平風"},		
		{"voiceid":160,"wifevoice":160,"type":1,"wifetext":"姑娘對我哥哥有多真，我就對姑娘有多好！"},		
		{"voiceid":180,"wifevoice":180,"type":1,"wifetext":"別整天懶懶散散的，有空就陪本王打獵去！"},		
		{"voiceid":200,"wifevoice":200,"type":1,"wifetext":"如果要在皇位和你之間做出抉擇，那我選兩個都要！"},		
		{"voiceid":220,"wifevoice":220,"type":1,"wifetext":"待我榮登大寶，必讓你母儀天下！"},		
		{"voiceid":240,"wifevoice":240,"type":1,"wifetext":"今兒有空嗎？本王想聽你彈奏一曲《鳳求凰》…"},		
		{"voiceid":260,"wifevoice":260,"type":1,"wifetext":"你總算來了，趕緊的陪本王賭上幾把！"},		
		{"voiceid":280,"wifevoice":0,"type":1,"wifetext":"縞素臨江誓滅胡，雄師十萬氣吞吳。試看天塹投鞭渡，不信中原不姓朱"},		
		{"voiceid":300,"wifevoice":0,"type":1,"wifetext":"此夕會盟天下合，四海招徠盡姓洪，金針取血同立誓，兄弟齊心要合同！"},		
		{"voiceid":320,"wifevoice":0,"type":1,"wifetext":"君子報仇十年不晚，女子報仇一生永恆！"},		
		{"voiceid":340,"wifevoice":0,"type":1,"wifetext":"哪天你沒地方去了，就來漠南和我做一輩子的姐妹"},		
		{"voiceid":360,"wifevoice":0,"type":1,"wifetext":"無生老母，真空家鄉；劫富濟貧，日月複來！"},		
		{"voiceid":380,"wifevoice":0,"type":1,"wifetext":"日月旗重進北京之日，便是我與你雙宿雙飛之時！"},		
		{"voiceid":400,"wifevoice":0,"type":1,"wifetext":"書中自有顏如玉，古人誠不欺我"},		
		{"voiceid":420,"wifevoice":0,"type":1,"wifetext":"京城雲橘波詭，你我還是儘早遠離為好"},		
		{"voiceid":440,"wifevoice":0,"type":1,"wifetext":"天家自古一條路，只願你為同行人！"},		
		{"voiceid":460,"wifevoice":460,"type":1,"wifetext":"有沒有興趣和我平分這個世界？"},		
		{"voiceid":480,"wifevoice":0,"type":1,"wifetext":"知我者謂我心憂，不知我者謂我何求！"},		
		{"voiceid":500,"wifevoice":500,"type":1,"wifetext":"你既和本王結為知己，就少和其他人來往！"},		
		{"voiceid":520,"wifevoice":520,"type":1,"wifetext":"叛臣之女，豈能安置在後宮之中？"},		
		{"voiceid":530,"wifevoice":530,"type":1,"wifetext":"賑糧與否，全憑本王心情，你能奈我何？"},		
		{"voiceid":540,"wifevoice":540,"type":1,"wifetext":"你，過來讓本王看看。"},		
		{"voiceid":550,"wifevoice":0,"type":1,"wifetext":"人心難辨，吾以觀複，唯有你，本王看不透。"},		
		{"voiceid":560,"wifevoice":0,"type":1,"wifetext":"是你讓我知道世上竟還有知己這種關係的存在。"},		
		{"voiceid":550,"wifevoice":0,"type":1,"wifetext":"人心難辨，吾以觀複，唯有你，本王看不透。"},		
		{"voiceid":560,"wifevoice":0,"type":1,"wifetext":"是你讓我知道世上竟還有知己這種關係的存在。"},		
		{"voiceid":570,"wifevoice":0,"type":1,"wifetext":"秋風清，秋月明，落葉聚還散，寒鴉棲複驚。"},		
		{"voiceid":580,"wifevoice":0,"type":1,"wifetext":"相逢紅塵內，高揖黃金鞭。"}		
