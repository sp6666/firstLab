	"heroVoice":[
		{"voiceid":1,"herovoice":1,"type":1,"herotext":"泽门之皙，实兴我役，邑中之黔，实慰我心。"},
		{"voiceid":10,"herovoice":10,"type":1,"herotext":"销甲事春农，万里孤征，终古谋谟齐稷契。"},
		{"voiceid":20,"herovoice":20,"type":1,"herotext":"敏而好学可为文，授之以政无不达。"},
		{"voiceid":30,"herovoice":30,"type":1,"herotext":"泥芹有宿根，一寸嗟独在，雪芹何时动，春鸠行可脍。"},
		{"voiceid":33,"herovoice":33,"type":1,"herotext":"记住，我是礼亲王，两红旗的旗主！"},
		{"voiceid":40,"herovoice":40,"type":1,"herotext":"人生若只如初见，何事秋风悲画扇。"},
		{"voiceid":50,"herovoice":50,"type":1,"herotext":"千古英雄死遗恨，封侯庙食更何心。"},
		{"voiceid":60,"herovoice":60,"type":1,"herotext":"礼乐法而不说，诗书故而不切，春秋约而不速。"},
		{"voiceid":70,"herovoice":70,"type":1,"herotext":"四时不谢之兰，百节长青之竹，万古不败之石，千秋不变之人。"},
		{"voiceid":80,"herovoice":80,"type":1,"herotext":"今后，这宫里生活不比外头，无论在哪里都需如履薄冰。"},
		{"voiceid":81,"herovoice":81,"type":2,"herotext":"若有什么难出，尽管和我说便是。"},
		{"voiceid":82,"herovoice":82,"type":2,"herotext":"没……没什么，只是想来看看你。"},
		{"voiceid":83,"herovoice":83,"type":2,"herotext":"今日气候适宜，走，陪本王射箭去！"},
		{"voiceid":90,"herovoice":90,"type":1,"herotext":"国家大事重于一切，真难得，你居然能有此觉悟。"},
		{"voiceid":91,"herovoice":91,"type":1,"herotext":"百姓安危顾然重要，但你只是个弱女子，莫要逞强。"},
		{"voiceid":92,"herovoice":92,"type":1,"herotext":"你啊你，何时才能让本王省点心。"},
		{"voiceid":93,"herovoice":93,"type":1,"herotext":"若是你愿意，本王可当你永远的后盾，为你遮风挡雨。"},
		{"voiceid":100,"herovoice":100,"type":1,"herotext":"等闲识得东风面，万紫千红总是春……"},
		{"voiceid":101,"herovoice":101,"type":2,"herotext":"天凉，可别在外头待太久……"},
		{"voiceid":102,"herovoice":102,"type":2,"herotext":"只是许久不见你，想看看你近来过得如何……"},
		{"voiceid":103,"herovoice":103,"type":2,"herotext":"爱也好，恨也罢，就让时间来证明吧。"},
		{"voiceid":110,"herovoice":110,"type":1,"herotext":"今日气候适宜，姑娘可愿陪本王一同前往灵囿欣赏南国孔雀？"},
		{"voiceid":111,"herovoice":111,"type":1,"herotext":"本王前些时日赠与你的青凤裘，你可还喜欢？"},
		{"voiceid":112,"herovoice":112,"type":1,"herotext":"你总是能看穿我的所思所想，是本王低估你了。"},
		{"voiceid":113,"herovoice":113,"type":1,"herotext":"把手给我，本王带你去看看这冬日的第一场雪。"},
		{"voiceid":120,"herovoice":120,"type":1,"herotext":"怎么，不相信本王？"},
		{"voiceid":121,"herovoice":121,"type":2,"herotext":"哈哈哈哈，这几日不见，你可曾思念我？"},
		{"voiceid":122,"herovoice":122,"type":2,"herotext":"本王的心，永远留有你的位置。"},
		{"voiceid":123,"herovoice":123,"type":2,"herotext":"这初见平平无奇，可你越看…却越让本王心动。"},
		{"voiceid":130,"herovoice":130,"type":1,"herotext":"你可别太担心，本王一向对女人温柔。"},
		{"voiceid":131,"herovoice":131,"type":1,"herotext":"我对你的真心，你难道看不出来么？"},
		{"voiceid":132,"herovoice":132,"type":1,"herotext":"告诉本王，想要占据你的心，本王……还该做些什么？"},
		{"voiceid":133,"herovoice":133,"type":1,"herotext":"认命吧，你的整个人整个心，终将属于本王。"},
		{"voiceid":140,"herovoice":140,"type":1,"herotext":"愿有一日，可与哥哥们一同把酒言欢。"},
		{"voiceid":141,"herovoice":141,"type":2,"herotext":"本王平日舞刀弄枪惯了，可没吓到姑娘吧。"},
		{"voiceid":142,"herovoice":142,"type":2,"herotext":"姑娘可知，本王久经沙场，生死早已置之度外。"},
		{"voiceid":143,"herovoice":143,"type":2,"herotext":"和你说说话，本王这心里便畅快多了。"},
		{"voiceid":150,"herovoice":150,"type":1,"herotext":"我还是头一次，见到像你这么好看的姑娘。"},
		{"voiceid":151,"herovoice":151,"type":1,"herotext":"舞刀弄枪的，我可是最拿手了。别怕，我保护你！"},
		{"voiceid":152,"herovoice":152,"type":1,"herotext":"若是你想，本王也可放下刀枪，陪你一同赏花看画。"},
		{"voiceid":153,"herovoice":153,"type":1,"herotext":"这江山，以前是为了八哥守着，现在，我想也替你守着。"},
		{"voiceid":160,"herovoice":160,"type":1,"herotext":"今儿有空吗？本王想听你弹奏一曲《凤求凰》…"},
		{"voiceid":162,"herovoice":162,"type":2,"herotext":"姑娘是不是也觉得？八哥很厉害！"},
		{"voiceid":163,"herovoice":163,"type":2,"herotext":"你这眉头都快拧成球了，快给本王笑一个！"},
		{"voiceid":164,"herovoice":164,"type":2,"herotext":"其实这宫里啊，也没你想的这么恐怖，这不？有我在呢！"},
		{"voiceid":170,"herovoice":170,"type":1,"herotext":"好一个巾帼英雄，本王深感钦佩！"},
		{"voiceid":171,"herovoice":171,"type":1,"herotext":"这天天听八哥说你的故事，我的耳朵都起茧了。"},
		{"voiceid":172,"herovoice":172,"type":1,"herotext":"姑娘既为八哥之妹，那也是本王家人，自当为你倾尽所有。"},
		{"voiceid":173,"herovoice":173,"type":1,"herotext":"不曾想，我一向寡淡的生命里，竟闯进了你……"},
		{"voiceid":180,"herovoice":180,"type":1,"herotext":"你总算来了，赶紧的！陪本王赌上几把！"},
		{"voiceid":181,"herovoice":181,"type":2,"herotext":"这日子闲，小赌怡情，小赌怡情嘛！"},
		{"voiceid":182,"herovoice":182,"type":2,"herotext":"本王当你是朋友，才跟你说这些的，你可别不领情！"},
		{"voiceid":183,"herovoice":183,"type":2,"herotext":"你今儿气色不错嘛！可有什么好事说来听听~"},
		{"voiceid":190,"herovoice":190,"type":1,"herotext":"凡事莫要冲动，有事可找本王商量。"},
		{"voiceid":191,"herovoice":191,"type":1,"herotext":"这皇宫里人才辈出，本王一身才情，也只能给你看了。"},
		{"voiceid":192,"herovoice":192,"type":1,"herotext":"想来这世间，竟没人如你般了解本王了。"},
		{"voiceid":193,"herovoice":193,"type":1,"herotext":"本王赌了这么久，何时才能赌一把……你的心？"},
		{"voiceid":200,"herovoice":200,"type":1,"herotext":"这有些事啊，可没你想的那么简单。"},
		{"voiceid":201,"herovoice":201,"type":2,"herotext":"本王只不过想，一生一世一双人罢了。"},
		{"voiceid":202,"herovoice":202,"type":2,"herotext":"醉过方知酒浓，爱过方知情重。"},
		{"voiceid":203,"herovoice":203,"type":2,"herotext":"若她有你对我一半好，那该多好……"},
		{"voiceid":210,"herovoice":210,"type":1,"herotext":"这一次，我是真的要忘了。"},
		{"voiceid":211,"herovoice":211,"type":1,"herotext":"是你让我忘却了那些不高兴的事，说来…还得感谢你呢。"},
		{"voiceid":212,"herovoice":212,"type":1,"herotext":"你为何，总是出现在本王的心头？"},
		{"voiceid":213,"herovoice":213,"type":1,"herotext":"这寒冬夜本该觉得寒冷，但有你在本王身旁，竟觉得甚是温暖。"},
		{"voiceid":220,"herovoice":220,"type":1,"herotext":"本王还有事要忙，姑娘，先回吧。"},
		{"voiceid":221,"herovoice":221,"type":2,"herotext":"这一去便不知多久才能回来，可别太想我。"},
		{"voiceid":222,"herovoice":222,"type":2,"herotext":"喏~才带回来的桂花糕，快尝尝！"},
		{"voiceid":223,"herovoice":223,"type":2,"herotext":"本王不在的这些时日，你可别多看其他男人！"},
		{"voiceid":230,"herovoice":230,"type":1,"herotext":"姑娘，你可不能忘了本王帮过你的恩情啊。"},
		{"voiceid":231,"herovoice":231,"type":1,"herotext":"本王真是有幸，竟结识了如此聪慧的女子。"},
		{"voiceid":232,"herovoice":232,"type":1,"herotext":"本王决定，今后与姑娘患难与共了！"},
		{"voiceid":233,"herovoice":233,"type":1,"herotext":"想不到本王的心，有一日也会因姑娘而狂跳不止。"},
		{"voiceid":240,"herovoice":240,"type":1,"herotext":"嗯？你找我……有什么事吗？"},
		{"voiceid":241,"herovoice":241,"type":2,"herotext":"嗯，来了？"},
		{"voiceid":242,"herovoice":242,"type":2,"herotext":"人生如棋，这个局你可能破？"},
		{"voiceid":243,"herovoice":243,"type":2,"herotext":"曲通人心，与你是，与我也是。"},
		{"voiceid":250,"herovoice":250,"type":1,"herotext":"如果美色时时都有用的话，这宫里的有些事，便简单许多。"},
		{"voiceid":251,"herovoice":251,"type":1,"herotext":"你放心，就算是为了我自己，我是不会丢下你一个人的。"},
		{"voiceid":252,"herovoice":252,"type":1,"herotext":"可以多考虑考虑自己吗，可……可别再受伤了！"},
		{"voiceid":253,"herovoice":253,"type":1,"herotext":"本王的心……也是会因你而疼的。"},
		{"voiceid":260,"herovoice":260,"type":1,"herotext":"你我二人今朝相见，竟已是这般陌生……"},
		{"voiceid":261,"herovoice":261,"type":2,"herotext":"看这春风欲度桃花落，你我二人却回不去了…"},
		{"voiceid":262,"herovoice":262,"type":2,"herotext":"我不在的日子，你可要照顾好自己。"},
		{"voiceid":263,"herovoice":263,"type":2,"herotext":"待我君临天下，定赏你这一世的繁华。"},
		{"voiceid":280,"herovoice":280,"type":1,"herotext":"行遍江南水与山，柳舒花放鸟绵蛮。"},
		{"voiceid":290,"herovoice":290,"type":1,"herotext":"妻子岂应关大计?英雄无奈是多情！"},
		{"voiceid":300,"herovoice":300,"type":1,"herotext":"治天下在得民心，士为秀民，士心得，则民心得矣。"},
		{"voiceid":310,"herovoice":310,"type":1,"herotext":"本官掌仪天下之政，何人不服？"},
		{"voiceid":320,"herovoice":320,"type":1,"herotext":"本王只想独善其身，创作出更多美妙的乐曲。"},
		{"voiceid":321,"herovoice":321,"type":2,"herotext":"这笛声，你可还喜欢？"},
		{"voiceid":322,"herovoice":322,"type":2,"herotext":"有些说不出口的话，笛声可替本王诉说。"},
		{"voiceid":323,"herovoice":323,"type":2,"herotext":"明日有场礼乐表演，不知姑娘可愿陪本王同去？"},
		{"voiceid":330,"herovoice":330,"type":1,"herotext":"快来，我新作了一首曲子，你听听，可还喜欢？"},
		{"voiceid":331,"herovoice":331,"type":1,"herotext":"不知从何时起，第一时间就想和你分享好曲子。"},
		{"voiceid":332,"herovoice":332,"type":1,"herotext":"原来，这便是心动的感觉……"},
		{"voiceid":333,"herovoice":333,"type":1,"herotext":"你可知，这是本王第一次如此用心的对待一个女人。"},
		{"voiceid":340,"herovoice":340,"type":1,"herotext":"有的人即使脸上在笑，心中的痛，却是半分也不少的。"},
		{"voiceid":350,"herovoice":350,"type":1,"herotext":"姑娘可是我见过的人中，最善良的那个。"},
		{"voiceid":351,"herovoice":351,"type":1,"herotext":"这紫禁城，远比想象中来的凶险，姑娘要多加小心。"},
		{"voiceid":352,"herovoice":352,"type":1,"herotext":"属下愿格格一生一帆风顺，再不受苦。"},
		{"voiceid":353,"herovoice":353,"type":1,"herotext":"若可以，属下愿牺牲自己，为格格赴汤蹈火，在所不辞！"},
		{"voiceid":360,"herovoice":360,"type":1,"herotext":"像你这样，荣华富贵都入不了眼的姑娘，可真是稀奇。"},
		{"voiceid":370,"herovoice":370,"type":1,"herotext":"你要知道，江山对我来说，远比儿女情长重要得多。"},
		{"voiceid":371,"herovoice":371,"type":1,"herotext":"你……能够原谅我吗？"},
		{"voiceid":372,"herovoice":372,"type":1,"herotext":"只是夜深人静的时候，时常会想起你罢了。"},
		{"voiceid":373,"herovoice":373,"type":1,"herotext":"失去你的日子，本王夜夜无法安稳入眠。只怕你…是永远回不来了。"},
		{"voiceid":380,"herovoice":380,"type":1,"herotext":"这天下，没有什么是我得不到的。"},
		{"voiceid":400,"herovoice":400,"type":1,"herotext":"哟，这太阳打西边出来了呀，我当是谁呢？"},
		{"voiceid":420,"herovoice":420,"type":1,"herotext":"赈粮与否，全凭本王心情，你能奈我何？"},
		{"voiceid":430,"herovoice":430,"type":1,"herotext":"满洲乃大清龙兴之地，岂容蛮夷放肆！"},
		{"voiceid":450,"herovoice":450,"type":1,"herotext":"有没有兴趣和我平分这个世界？"},
		{"voiceid":460,"herovoice":460,"type":1,"herotext":"你，过来让本王看看。"},
		{"voiceid":480,"herovoice":0,"type":1,"herotext":"谦谦君子，卑以自牧也"},
		{"voiceid":490,"herovoice":0,"type":1,"herotext":"我命由我，不由天！"},
		{"voiceid":500,"herovoice":0,"type":1,"herotext":"不谋全局者，不足以谋一域"},
		{"voiceid":510,"herovoice":0,"type":1,"herotext":"遇到危险时，就站到我的身后"},
		{"voiceid":520,"herovoice":0,"type":1,"herotext":"见你面有愁容，最近可有什么烦心事?"},
		{"voiceid":530,"herovoice":0,"type":1,"herotext":"十里长亭，望眼欲穿"},
		{"voiceid":540,"herovoice":0,"type":1,"herotext":"君王是人，也有七情六欲。"},
		{"voiceid":550,"herovoice":0,"type":1,"herotext":"  看到你，心情便明快了起来。"},
		{"voiceid":560,"herovoice":0,"type":1,"herotext":"君子有诸己而后求诸人，无诸己而后非诸人"}
	],
	"wifeVoice":[
		{"voiceid":1,"wifevoice":1,"type":1,"wifetext":"我只想和你策马奔腾，共享尘世繁华！"},
		{"voiceid":20,"wifevoice":20,"type":1,"wifetext":"我虽卑微，却仍可助你一臂之力！"},
		{"voiceid":21,"wifevoice":21,"type":1,"wifetext":"有时……我总觉得你是另一个我。"},
		{"voiceid":22,"wifevoice":22,"type":1,"wifetext":"开心的时候要想起我，难过的时候也别忘了我。"},
		{"voiceid":23,"wifevoice":23,"type":1,"wifetext":"你我二人相互扶持，这宫里再苦再累，我都能受。"},
		{"voiceid":40,"wifevoice":40,"type":1,"wifetext":"只要姑娘心里有我一个位置，培盛就知足了。"},
		{"voiceid":60,"wifevoice":60,"type":1,"wifetext":"就算是天定的良缘，也会很辛苦，我有心里准备！"},
		{"voiceid":80,"wifevoice":80,"type":1,"wifetext":"若非家门不幸，我也遇不到你，这也算祸兮福所依吧~"},
		{"voiceid":100,"wifevoice":0,"type":1,"wifetext":"如果宗室都能如姑娘这般自律，天下何愁不太平？"},
		{"voiceid":120,"wifevoice":0,"type":1,"wifetext":"哼，殿下这么久不来看我，本宫不理你了！"},
		{"voiceid":140,"wifevoice":0,"type":1,"wifetext":"天日昭昭，坠月平风"},
		{"voiceid":160,"wifevoice":160,"type":1,"wifetext":"姑娘对我哥哥有多真，我就对姑娘有多好！"},
		{"voiceid":180,"wifevoice":180,"type":1,"wifetext":"别整天懒懒散散的，有空就陪本王打猎去！"},
		{"voiceid":200,"wifevoice":200,"type":1,"wifetext":"如果要在皇位和你之间做出抉择，那我选两个都要！"},
		{"voiceid":220,"wifevoice":220,"type":1,"wifetext":"待我荣登大宝，必让你母仪天下！"},
		{"voiceid":240,"wifevoice":240,"type":1,"wifetext":"今儿有空吗？本王想听你弹奏一曲《凤求凰》…"},
		{"voiceid":260,"wifevoice":260,"type":1,"wifetext":"你总算来了，赶紧的陪本王赌上几把！"},
		{"voiceid":280,"wifevoice":0,"type":1,"wifetext":"缟素临江誓灭胡，雄师十万气吞吴。试看天堑投鞭渡，不信中原不姓朱"},
		{"voiceid":300,"wifevoice":0,"type":1,"wifetext":"此夕会盟天下合，四海招徕尽姓洪，金针取血同立誓，兄弟齐心要合同！"},
		{"voiceid":320,"wifevoice":0,"type":1,"wifetext":"君子报仇十年不晚，女子报仇一生永恒！"},
		{"voiceid":340,"wifevoice":0,"type":1,"wifetext":"哪天你没地方去了，就来漠南和我做一辈子的姐妹"},
		{"voiceid":360,"wifevoice":0,"type":1,"wifetext":"无生老母，真空家乡；劫富济贫，日月复来！"},
		{"voiceid":380,"wifevoice":0,"type":1,"wifetext":"日月旗重进北京之日，便是我与你双宿双飞之时！"},
		{"voiceid":400,"wifevoice":0,"type":1,"wifetext":"书中自有颜如玉，古人诚不欺我"},
		{"voiceid":420,"wifevoice":0,"type":1,"wifetext":"京城云橘波诡，你我还是尽早远离为好"},
		{"voiceid":440,"wifevoice":0,"type":1,"wifetext":"天家自古一条路，只愿你为同行人！"},
		{"voiceid":460,"wifevoice":460,"type":1,"wifetext":"有没有兴趣和我平分这个世界？"},
		{"voiceid":480,"wifevoice":0,"type":1,"wifetext":"知我者谓我心忧，不知我者谓我何求！"},
		{"voiceid":500,"wifevoice":500,"type":1,"wifetext":"你既和本王结为知己，就少和其他人来往！"},
		{"voiceid":520,"wifevoice":520,"type":1,"wifetext":"叛臣之女，岂能安置在后宫之中？"},
		{"voiceid":530,"wifevoice":530,"type":1,"wifetext":"赈粮与否，全凭本王心情，你能奈我何？"},
		{"voiceid":540,"wifevoice":540,"type":1,"wifetext":"你，过来让本王看看。"},
		{"voiceid":550,"wifevoice":0,"type":1,"wifetext":"人心难辨，吾以观复，唯有你，本王看不透。"},
		{"voiceid":560,"wifevoice":0,"type":1,"wifetext":"是你让我知道世上竟还有知己这种关系的存在。"},
		{"voiceid":550,"wifevoice":0,"type":1,"wifetext":"人心难辨，吾以观复，唯有你，本王看不透。"},
		{"voiceid":560,"wifevoice":0,"type":1,"wifetext":"是你让我知道世上竟还有知己这种关系的存在。"},
		{"voiceid":570,"wifevoice":0,"type":1,"wifetext":"秋风清，秋月明，落叶聚还散，寒鸦栖复惊。"},
		{"voiceid":580,"wifevoice":0,"type":1,"wifetext":"相逢红尘内，高揖黄金鞭。"}		
	],
